        <TextBox Height="23" HorizontalAlignment="Left" Margin="12,26,0,0" Name="idBox" VerticalAlignment="Top" Width="161" IsEnabled="False" />
        <Button Content="Spremi" Height="35" HorizontalAlignment="Left" Margin="40,238,0,0" Name="saveButton" VerticalAlignment="Top" Width="107" Click="saveButton_Click" />
    </Grid>
</Window>
