            <TextBox Height="23" HorizontalAlignment="Left" Margin="1,156,0,0" Name="vrijemeBox" VerticalAlignment="Top" Width="158" />
            <Label Content="ID" Height="28" HorizontalAlignment="Left" Margin="-3,0,0,0" Name="label2" VerticalAlignment="Top" />
            <TextBox Height="23" HorizontalAlignment="Left" IsEnabled="False" Margin="2,22,0,0" Name="idBox" VerticalAlignment="Top" Width="157" />
        </Grid>
    </Grid>
</Window>
